node{
   stage('SCM Checkout'){
     git 'https://github.com/dileep2018/newproject'
   }
   stage('sonarqube analysis'){
       // Get maven home path
       def mvnHome = tool name: 'maven-3', type: 'maven'
       withSonarQubeEnv('sonar') {
       sh "${mvnHome}/bin/mvn sonar:sonar"
       }
   }      	
}
